@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_2021066_yy6kaufixci.eot?t=1598000514412'); /* IE9 */
  src: url('//at.alicdn.com/t/font_2021066_yy6kaufixci.eot?t=1598000514412#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_2021066_yy6kaufixci.woff?t=1598000514412') format('woff'),
  url('//at.alicdn.com/t/font_2021066_yy6kaufixci.ttf?t=1598000514412') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_2021066_yy6kaufixci.svg?t=1598000514412#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconmobile1:before {
  content: "\e600";
}

.iconjifen:before {
  content: "\e64a";
}

.iconpingtai:before {
  content: "\e68a";
}

.app-padding{
  padding: 35rpx;
  box-sizing: border-box;
}
/* flex布局开始 */
.flex{
	display: flex;
}
.flex-col{
	flex-direction: column;
}
.flex-row{
	flex-direction: row;
}
.flex-center{
	align-items: center;
	justify-content: center;
}
.flex-star{
	align-items: center;
	justify-content: flex-start;
}
.flex-end{
	align-items: center;
	justify-content: flex-end;
}
.flex-between{
	align-items: center;
	justify-content: space-between;
}
.flex-around{
	align-items: center;
	justify-content: space-around;
}
.flex-all{
  flex: 1;
}
.flex-warp{
	flex-wrap: wrap;
}
/* flex布局结束 */

/* 通用高度类名开始 */
.full-height{
  height: 100%;
}
.full-width{
	width: 100%;
}
/* 通用高度类名结束 */

/* 滚动相关类名开始 */
