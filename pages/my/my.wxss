/* pages/my/my.wxss */
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  background:rgba(255,255,255,1);
  box-shadow:0px 6px 16px 0px rgba(63,116,212,0.18);
  border: 4px solid white;
}
.my-container{
  padding: 0 25rpx;
  margin-top: -160rpx;
}
.userinfo-nickname {
  color: #666666;
  padding-top: 30rpx;
}
.my-handel-item{
  margin:10rpx 0;
}
.my-handel-item .van-cell {
  padding-left: 0;
  padding-right: 0;
}
.my-handel-item .van-cell::after{
  left:0;
  right:0
}
.my-handel-container{
  padding: 50rpx 10rpx;
}
.my-icon{
  margin-right: 10px;
}
